player.Chatted:Connect(function(message)
    if message:lower() == "im vengeance" then
        local A_1 = "wear"
local A_2 = 3049284724
Event:FireServer(A_1, A_2)
local A_1 = "wear"
local A_2 = 3049285265
Event:FireServer(A_1, A_2)
local A_1 = "wear"
local A_2 = 10647971081
Event:FireServer(A_1, A_2)
    end
end)

