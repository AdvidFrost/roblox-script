    print("yes overseer")
    local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
local gobackto = hrp.CFrame
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local localPlayer = Players.LocalPlayer
local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
local rootPart = character:WaitForChild("HumanoidRootPart")
local targetPlayerName = username -- Replace this with the target player's name

local circleDiameter = 60
local circleRadius = circleDiameter / 2
local incrementAngle = 1 -- Teleport angle increment in degrees
local circleSpeed = 2 -- Set the speed at which you circle around the target player (in rotations per second)
local remoteEventInterval = 0.3 -- Set the interval for executing the remote event

local circleCompleted = false

local function findTargetPlayer()
    for _, player in ipairs(Players:GetPlayers()) do
        if player.Name == targetPlayerName then
            return player
        end
    end
    return nil
end

local function fireRemoteEventLoop()
    while not circleCompleted do
        local A_1 = {
            ["Goal"] = "VIP Server Power",
            ["power"] = "Spawn Dummy"
        }
        local Event = game:GetService("Workspace").Live.LOCALPLAYEERRRRRRRRRR.Communicate
        Event:FireServer(A_1)
        wait(remoteEventInterval)
    end
end

local function circleTarget()
    local targetPlayer = findTargetPlayer()

    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local targetPosition = targetPlayer.Character.HumanoidRootPart.Position
        local timePerRotation = 1 / circleSpeed
        local timePerIncrement = timePerRotation * (incrementAngle / 360)

        for angle = 0, 360, incrementAngle do
            local newPosition = Vector3.new(
                targetPosition.X + circleRadius * math.cos(math.rad(angle)),
                targetPosition.Y,
                targetPosition.Z + circleRadius * math.sin(math.rad(angle))
            )
            rootPart.CFrame = CFrame.new(newPosition) * CFrame.Angles(0, math.rad(angle + 90), 0)
            wait(timePerIncrement)
        end
        
        circleCompleted = true 
    else
        warn("Target player not found or character not available")
    end
end

spawn(fireRemoteEventLoop)
circleTarget()
wait(0.2)
hrp.CFrame = gobackto
