    local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
local gobackto = hrp.CFrame
local function getRandomPosition(center, radius, yMin, yMax)
    local angle = math.random() * math.pi * 2
    local distance = math.random() * radius
    local x = center.x + math.cos(angle) * distance
    local z = center.z + math.sin(angle) * distance
    local y = math.random(yMin, yMax)
    return Vector3.new(x, y, z)
end

local function teleportAndExecuteRemote(player, position)
    player.Character:SetPrimaryPartCFrame(CFrame.new(position))

    local A_1 = {
        ["Goal"] = "VIP Server Power",
        ["power"] = "Spawn Dummy"
    }
    local Event = game:GetService("Workspace").Live.LOCALPLAYEERRRRRRRRRR.Communicate
    Event:FireServer(A_1)
end

local startPosition = Vector3.new(178, 643, -15)
local teleportDistance = 300
local yMin = 1000
local yMax = 1400

local player = game.Players.LocalPlayer
local startTime = os.clock()
local elapsedTime = 0
local delay = 0.25
local duration = 15

while elapsedTime < duration do
    local randomPosition = getRandomPosition(startPosition, teleportDistance, yMin, yMax)
    teleportAndExecuteRemote(player, randomPosition)
    wait(delay)
    elapsedTime = os.clock() - startTime
end
wait(2)
hrp.CFrame = gobackto
