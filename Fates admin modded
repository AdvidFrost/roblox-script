                local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local ChatRemote = ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest

local voteCounter = 0
local voteList = {}
local requiredVotes = math.round(Players.NumPlayers * 0.75)
local message = "A battle royal vote has been started, 0/" .. requiredVotes .. " votes needed, type !vote yes to accept the vote. This message expires in 60 seconds."
local voteActive = true

local function onPlayerAdded(player)
    player.Chatted:Connect(function(chatMessage)
        if chatMessage == "!vote yes" and not voteList[player.UserId] and voteActive then
            voteList[player.UserId] = true
            voteCounter = voteCounter + 1

            if voteCounter >= requiredVotes then
                voteActive = false
                ChatRemote:FireServer("Battle royal vote successful!", "All")
local A_1 =  {
	["Goal"] = "VIP Server Power", 
	["power"] = "Battle Royale"
}
local Event = game:GetService("Workspace").Live.LOCALPLAYEERRRRRRRRRR.Communicate
Event:FireServer(A_1)

            else
                ChatRemote:FireServer("Vote registered. Current vote count: " .. voteCounter .. "/" .. requiredVotes, "All")
            end
        end
    end)
end

local function init()
    for _, player in ipairs(Players:GetPlayers()) do
        onPlayerAdded(player)
    end
    Players.PlayerAdded:Connect(onPlayerAdded)
end

local function startVote()
    ChatRemote:FireServer(message, "All")
    wait(60)

    if voteCounter < requiredVotes and voteActive then
        voteActive = false
        ChatRemote:FireServer("Vote expired. Not enough votes.", "All")
    end
end

init()
startVote()

