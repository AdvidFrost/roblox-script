local theboss = {"Advidfrost5"}

local playerName = game.Players.LocalPlayer.Name
local isBoss = false

for i, v in ipairs(theboss) do
    if playerName == v then
        isBoss = true
        break
    end
end

if isBoss then
    print("Wassup boss")
else
    print("Ay boy you aint the boss")
end

local folderName = "overseers"
local fileName = folderName .. "/players.txt"
local playerName = game.Players.LocalPlayer.Name
local isOverseer = false

-- Check if the folder and file exist, if not create them
if not isfolder(folderName) then
    makefolder(folderName)
end

if not isfile(fileName) then
    writefile(fileName, "")
end

-- Read the file content and split it into a table with usernames
local fileContent = readfile(fileName)
local overseers = {}
for username in fileContent:gmatch("([^\n]+)") do
    table.insert(overseers, username)
end

-- Check if the local player is an overseer
for i, v in ipairs(overseers) do
    if playerName == v then
        isOverseer = true
        break
    end
end

-- Print the result
if isOverseer then
    print("yes overseer")
else
    print("not overseer")
end

local function onChatMessage(speaker, message)
    local command = "!kick"
    
    if string.sub(message, 1, #command) == command then
        local argument = string.sub(message, #command + 2)
        if #argument > 0 then
            local partialTargetDisplayNameOrUsername = argument
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer


local function findTargetPlayer()
    local targetPlayer = nil
    
    for _, player in ipairs(Players:GetPlayers()) do
        if string.find(string.lower(player.DisplayName), string.lower(partialTargetDisplayNameOrUsername), 1, true) or
           string.find(string.lower(player.Name), string.lower(partialTargetDisplayNameOrUsername), 1, true) then
            targetPlayer = player
            break
        end
    end
    
    if targetPlayer then
        print("Target player found: " .. targetPlayer.Name)
    end
    
    return targetPlayer
end


local function sendKickCommand()
    
    if LocalPlayer then
        -- Find the target player
        local targetPlayer = findTargetPlayer()
        
        if targetPlayer then
            if targetPlayer == LocalPlayer then
                local ChatRemote = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
                ChatRemote:FireServer(("Don't bite the hand that feeds you!"), "All")
            else
                
                local command = "/kick " .. targetPlayer.Name
                
                local isOverseer = false
                
                local fileContent = readfile(fileName)
local overseers = {}
for username in fileContent:gmatch("([^\n]+)") do
    table.insert(overseers, username)
end

for i, v in ipairs(overseers) do
    if speaker == v then
        isOverseer = true
        break
    end
end

-- Print the result
if isOverseer then
    Players:Chat(command)
else
    local ChatRemote = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
                ChatRemote:FireServer(("You aren't an overseer, you can't use this command"), "All")
end
            end
        else
            local ChatRemote = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
                ChatRemote:FireServer(("Player not found."), "All")
        end
    end
end

sendKickCommand()
        end
    end
end

local function onPlayerAdded(player)
    player.Chatted:Connect(function(message)
        onChatMessage(player.Name, message)
    end)
end

for _, player in ipairs(game.Players:GetPlayers()) do
    onPlayerAdded(player)
end

game.Players.PlayerAdded:Connect(onPlayerAdded)


local function clearDummies()
	local A_1 = {
		["Goal"] = "VIP Server Power",
		["power"] = "Clear Dummies"
	}
	local Event = game:GetService("Workspace").Live.LOCALPLAYEERRRRRRRRRR.Communicate
	Event:FireServer(A_1)
end

--
local function commandhelp()
	while true do
	    local player = game.Players.LocalPlayer
		local ChatRemote = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
                ChatRemote:FireServer(("Hello, for more info how to use the free private server commands, type !help in chat."), "All")
		wait(240) -- Wait for 1 minute and 50 seconds
	end
end

-- Main function
local function main()
	while true do
		wait(400) -- Wait for 1 minute and 50 seconds
		local player = game.Players.LocalPlayer
		local ChatRemote = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
                ChatRemote:FireServer(("Dummies will be cleared in 10 seconds"), "All")
		wait(10) -- Wait for 10 seconds
		clearDummies()
	end
end




local Cooldowns = {}


local function onPlayerChatted(player, message)
    
    local lowerMessage = message:lower()

    
    if lowerMessage == "!nocooldown" or lowerMessage == "!instantultimate" or lowerMessage == "!giveallultimate" or lowerMessage == "!spawndummy" or lowerMessage == "!2xhealth" or lowerMessage == "!2xdamage"  or lowerMessage == "!10xdamage" or lowerMessage == "!10xhealth" or lowerMessage == "!battleroyal" or lowerMessage == "!votekick" or lowerMessage == "!fastregen" or lowerMessage == "!fastrun" or lowerMessage == "!help" or lowerMessage == "!page2" or lowerMessage == "!credits" or lowerMessage == "!page3" or lowerMessage == "!dummyrain" or lowerMessage == "!dummycircle" or lowerMessage == "!shutdown" or lowerMessage == "!ban" or lowerMessage == "!forcebattleroyal" or lowerMessage == "!cleardummies" then
        
        if not Cooldowns[player.UserId] or os.time() - Cooldowns[player.UserId] >= 5 then
            
            Cooldowns[player.UserId] = os.time()

            
            local username = player.Name

            
            print(username .. " activated the phrase: " .. message)

            if lowerMessage == "!nocooldown" then
                
                local A_1 = {
                    ["Goal"] = "VIP Server Power",
                    ["power"] = "No Cooldown"
                }
                local Event = game:GetService("Workspace").Live.LOCALPLAYEERRRRRRRRRR.Communicate
                Event:FireServer(A_1)
if _G.nocooldown == false then
    _G.nocooldown = true
    else
        _G.nocooldown = false
        end
                
